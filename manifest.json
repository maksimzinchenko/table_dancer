{
  "manifest_version": 3,
  "name": "Table Dancer",
  "description": "Easy table data manipulation",
  "version": "0.1",
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "action": {
    "default_popup": "options.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "js": ["scripts/tableDancer.js"],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
